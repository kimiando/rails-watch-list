<%# <%= @lists.inspect %>
<%# <%= @lists.all %>
<div class="banner">
  <div class="container">
    <h1>WatchList App</h1>
    <p>Lorem ipsum, dolor sit amet consectetur adipisicing elit. Voluptas, quo aperiam mollitia nulla cumque ut cum laudantium consequatur sint similique fugit perspiciatis debitis? Soluta, a pariatur repellendus rerum optio labore.</p>
    <%= link_to "Add a new list✍🏻", new_list_path, class: 'btn btn-primary' %>
  </div>
</div>
<div class="form">
  <ul class="list-group">
    <li class="list-group-item">
      <% @lists.each do |list| %>
        <p> <%= link_to list.name, "lists/#{list.id}" %> </p>
      <% end %>
    </li>
  </ul>
</div>
<div class="cards">
  <% @lists.each do |list| %>
    <div class="card" style="width: 30rem;">
      <div class="card-body ">
        <p> <%= link_to list.name, "lists/#{list.id}" %> </p>
      </div>
    </div>
  <% end %>
</div>
